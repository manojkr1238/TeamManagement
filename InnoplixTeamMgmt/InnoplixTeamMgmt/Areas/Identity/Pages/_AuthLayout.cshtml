<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title</title>

    <!-- Custom fonts for this template-->
    <link href="~/theme/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/theme/css/sb-admin-2.min.css" rel="stylesheet">

</head>

<body id="page-top">
    <!-- Topbar -->
    <nav class="navbar navbar-expand navbar-dark bg-primary topbar mb-4 static-top shadow">
        <!-- Sidebar - Brand -->
        <a class="sidebar-brand d-flex align-items-center justify-content-center" asp-controller="Default" asp-action="Index">
            <div class="sidebar-brand-text text-white mx-3"><h2>dynamiX</h2></div>
        </a>
        <ul class="navbar-nav ml-auto">
            <!-- Nav Item - Alerts -->
            <li class="nav-item dropdown no-arrow mx-1">
                <a class="btn btn-user text-white" href="/identity/account/login">Login</a>
            </li>
            <li class="nav-item dropdown no-arrow mx-1">
                <a class="btn btn-user text-white" href="/identity/account/register">Registration</a>
            </li>
        </ul>
    </nav>
        <div class="container-fluid">
            @RenderBody()
        </div>
        <!-- Bootstrap core JavaScript-->
        <script src="~/theme/vendor/jquery/jquery.min.js"></script>
        <script src="~/theme/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/theme/vendor/jquery-easing/jquery.easing.min.js"></script>

        <!-- Custom scripts for all pages-->
        <script src="~/theme/js/sb-admin-2.min.js"></script>
        @RenderSection("Scripts", required: false)
</body>

</html>